{
  "name": "@apphead/server",
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": "./src/index.ts",
    "./types": "./src/types.ts"
  },
  "description": "The server template",
  "repository": {
    "type": "git",
    "url": "<PLACEHOLDER>",
    "directory": "packages/server"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "check": "tsc -p tsconfig.json --noEmit",
    "test": "bun test"
  },
  "dependencies": {
    "@effect/platform": "latest",
    "@effect/platform-node": "latest",
    "@apphead/app": "workspace:^",
    "@apphead/auth": "workspace:^",
    "@apphead/ecommerce": "workspace:^",
    "effect": "latest"
  },
  "effect": {
    "generateExports": {
      "include": [
        "**/*.ts"
      ]
    },
    "generateIndex": {
      "include": [
        "**/*.ts"
      ]
    }
  },
  "devDependencies": {
    "@types/bun": "^1.2.20"
  }
}
